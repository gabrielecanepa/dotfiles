    ".eslintrc": "jsonc",
    ".gcloudignore": "dotenv",
    ".gitignore*": "ignore",
    ".gitprofile": "properties",
    ".jobs": "shellscript",
    ".json": "json",
    ".mdx": "markdown",
    ".pryrc": "ruby",
    ".remarkrc": "json",
    ".sequelizerc": "javascript",
